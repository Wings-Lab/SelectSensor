  1415                                           
  1416       154         82.0      0.5      0.0          summation = 0                                       # compute the mutual information
  1417     39578      23283.0      0.6      0.8          for trans in self.transmitters:
  1418     39424      30926.0      0.8      1.1              x = trans.hypothesis // self.grid_len
  1419     39424      22434.0      0.6      0.8              y = trans.hypothesis % self.grid_len
  1420     39424     217222.0      5.5      7.9              prob_xh = discretize_x[sensor_index, trans.hypothesis, :]
  1421     39424     457949.0     11.6     16.6              log_term = np.log2(prob_xh / prob_x)
  1422                                                       #from numpy import inf
  1423     39424     119857.0      3.0      4.3              log_term[log_term == np.inf] = 0
  1424     39424     426493.0     10.8     15.5              summation_term = self.grid_priori[x, y] * log_term * prob_xh
  1425     39424     977403.0     24.8     35.4              np.nan_to_num(summation_term, copy=False)
  1426                                           
  1427     39424     293968.0      7.5     10.7              summation += np.sum(summation_term)
  1428                                           
  1429       154         71.0      0.5      0.0          return summation

